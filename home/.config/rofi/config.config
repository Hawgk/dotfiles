! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #191919, #191919, #989898

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #191919, #d8d8d8, #191919, #191919, #d8d8d8
rofi.color-active:  #191919, #989898, #191919, #191919, #989898
rofi.color-urgent:  #191919, #7c7c7c, #191919, #191919, #7c7c7c

! Set the desired separator style
rofi.separator-style: solid
